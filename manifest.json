{
    "name": "quickly github1s",
    "version": "0.1",
    "manifest_version": 2,
    "description": "Launch github1s with shortcut keys",
    "icons": {
        "16": "quick_github1s.ico",
        "48": "quick_github1s.ico",
        "128": "quick_github1s.ico"
    },
    
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_icon": "quick_github1s.ico",
        "default_title": "quickly github1s"
    },
    "commands": {
        "quickLaunch": {
            "suggested_key": {
                "default": "Ctrl+Shift+1"
            },
            "global": true,
            "description": "Launch github1s"
        }
    },

    "permissions": [
        "tabs",
        "https://github.com/*"
    ]
}