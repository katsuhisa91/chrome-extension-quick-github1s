{
    "name": "shortcut to github1s",
    "version": "0.1",
    "manifest_version": 3,
    "description": "Launch github1s with Ctrl(Win)/Command(Mac)+Shift+1.",
    "icons": {
        "16": "quick_github1s.ico",
        "48": "quick_github1s.ico",
        "128": "quick_github1s.ico"
    },
    
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_icon": "quick_github1s.ico",
        "default_title": "shortcut to github1s"
    },
    "commands": {
        "quickLaunch": {
            "suggested_key": {
                "default": "Ctrl+Shift+1"
            },
            "global": true,
            "description": "Launch github1s"
        }
    },

    "permissions": [
        "activeTab"
    ],
    "host_permissions": [
        "http://github.com/",
        "*://*/*"
    ]
}